Cell[BoxData[
 RowBox[{
  RowBox[{"GetCKMParameters", "[", "unitaryMatrix_", "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "abss", ",", "args", ",", "c12", ",", "c13", ",", "c23", ",", "s12", ",",
       "s13", ",", "s23", ",", "delta", ",", "u1"}], "}"}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{"(*", " ", 
     RowBox[{
     "unitaryMatrix", " ", "must", " ", "be", " ", "3", " ", "by", " ", "3", 
      " ", "matrix"}], " ", "*)"}], "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"abss", "=", 
      RowBox[{"Map", "[", 
       RowBox[{"Abs", ",", "unitaryMatrix", ",", 
        RowBox[{"{", 
         RowBox[{"-", "1"}], "}"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"args", "=", 
      RowBox[{"Map", "[", 
       RowBox[{"Arg", ",", "unitaryMatrix", ",", 
        RowBox[{"{", 
         RowBox[{"-", "1"}], "}"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
     "\[IndentingNewLine]", 
     RowBox[{"(*", " ", 
      RowBox[{"CKM", " ", "parameters"}], " ", "*)"}], "\[IndentingNewLine]", 
     
     RowBox[{"s13", "=", 
      RowBox[{"abss", "[", 
       RowBox[{"[", 
        RowBox[{"1", ",", " ", "3"}], "]"}], "]"}]}], ";", 
     RowBox[{"c13", " ", "=", " ", 
      RowBox[{"Sqrt", "[", 
       RowBox[{"1", "-", 
        RowBox[{"s13", "^", "2"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"c12", " ", "=", " ", 
      RowBox[{
       RowBox[{"abss", "[", 
        RowBox[{"[", 
         RowBox[{"1", ",", " ", "1"}], "]"}], "]"}], "/", "c13"}]}], ";", 
     RowBox[{"s12", "=", 
      RowBox[{"Sqrt", "[", 
       RowBox[{"1", "-", 
        RowBox[{"c12", "^", "2"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"c23", " ", "=", " ", 
      RowBox[{
       RowBox[{"abss", "[", 
        RowBox[{"[", 
         RowBox[{"3", ",", " ", "3"}], "]"}], "]"}], "/", "c13"}]}], ";", 
     RowBox[{"s23", "=", 
      RowBox[{"Sqrt", "[", 
       RowBox[{"1", "-", 
        RowBox[{"c23", "^", "2"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
     "\[IndentingNewLine]", 
     RowBox[{"(*", " ", 
