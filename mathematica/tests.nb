              RowBox[{"args", "[", 
               RowBox[{"[", 
                RowBox[{"1", ",", "2"}], "]"}], "]"}], "+", 
              RowBox[{"2", " ", 
               RowBox[{"args", "[", 
                RowBox[{"[", 
                 RowBox[{"2", ",", " ", "3"}], "]"}], "]"}]}], "+", 
              RowBox[{"2", " ", 
               RowBox[{"args", "[", 
                RowBox[{"[", 
                 RowBox[{"3", ",", " ", "3"}], "]"}], "]"}]}]}], ")"}]}], 
           "]"}]}], " ", 
         RowBox[{"(", 
          RowBox[{
           RowBox[{
            RowBox[{"Exp", "[", 
             RowBox[{"I", 
              RowBox[{"(", " ", 
               RowBox[{
                RowBox[{"args", "[", 
                 RowBox[{"[", 
                  RowBox[{"2", ",", " ", "1"}], "]"}], "]"}], "+", " ", 
                RowBox[{"args", "[", 
                 RowBox[{"[", 
                  RowBox[{"3", ",", " ", "3"}], "]"}], "]"}]}], ")"}]}], 
             "]"}], "c23", " ", 
            RowBox[{"abss", "[", 
             RowBox[{"[", 
              RowBox[{"2", ",", "1"}], "]"}], "]"}]}], " ", "-", 
           RowBox[{
            RowBox[{"Exp", "[", 
             RowBox[{"I", 
              RowBox[{"(", 
               RowBox[{
                RowBox[{"args", "[", 
                 RowBox[{"[", 
                  RowBox[{"2", ",", " ", "3"}], "]"}], "]"}], "+", 
                RowBox[{"args", "[", 
                 RowBox[{"[", 
                  RowBox[{"3", ",", " ", "1"}], "]"}], "]"}]}], ")"}]}], 
             "]"}], "s23", " ", 
            RowBox[{"abss", "[", 
             RowBox[{"[", 
              RowBox[{"3", ",", "1"}], "]"}], "]"}]}]}], ")"}]}], 
        "\[IndentingNewLine]", "]"}]}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"(*", " ", 
      RowBox[{"solving", " ", "for", " ", "the", " ", "CP", " ", "phase"}], 
