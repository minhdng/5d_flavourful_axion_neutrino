              RowBox[{"[", 
               RowBox[{"1", ",", "2"}], "]"}], "]"}], "+", 
             RowBox[{"3", 
              RowBox[{"args", "[", 
               RowBox[{"[", 
                RowBox[{"1", ",", "3"}], "]"}], "]"}]}], "-", 
             RowBox[{"args", "[", 
              RowBox[{"[", 
               RowBox[{"2", " ", ",", "3"}], "]"}], "]"}], "-", 
             RowBox[{"args", "[", 
              RowBox[{"[", 
               RowBox[{"3", ",", "3"}], "]"}], "]"}]}], ")"}]}]}], ",", 
         RowBox[{"2", "Pi"}]}], "]"}], "-", "Pi"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"(*", " ", 
      RowBox[{"output", " ", 
       RowBox[{"{", 
