Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"FermionProfile", "[", 
    RowBox[{
     RowBox[{"c_", "?", "NumericQ"}], ",", "z_", ",", 
     RowBox[{"zuv_", ":", "1.001"}], ",", 
     RowBox[{"zir_", ":", " ", 
      RowBox[{"10.", "^", "8"}]}]}], "]"}], ":=", "\n", "\t", 
   RowBox[{"Piecewise", "[", 
    RowBox[{"{", "\n", "\t\t", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{
        RowBox[{
         RowBox[{"Sqrt", "[", 
          RowBox[{
           RowBox[{"(", 
            RowBox[{"1", "-", 
             RowBox[{"2", "c"}]}], ")"}], "/", 
           RowBox[{"(", 
            RowBox[{"2", 
             RowBox[{"(", 
              RowBox[{
               RowBox[{"zir", "^", 
                RowBox[{"(", 
                 RowBox[{"1", "-", 
                  RowBox[{"2", "c"}]}], ")"}]}], " ", "-", " ", 
               RowBox[{"zuv", "^", 
                RowBox[{"(", 
                 RowBox[{"1", "-", 
                  RowBox[{"2", "c"}]}], ")"}]}]}], ")"}]}], ")"}]}], "]"}], 
         " ", 
         RowBox[{"z", "^", 
          RowBox[{"(", 
           RowBox[{"2", "-", "c"}], ")"}]}]}], ",", " ", 
        RowBox[{"c", " ", "!=", " ", 
         RowBox[{"1", "/", "2"}]}]}], "}"}], ",", "\n", "\t\t", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"1", "/", 
         RowBox[{"Sqrt", "[", 
          RowBox[{"2", 
           RowBox[{"(", 
            RowBox[{
             RowBox[{"Log", "[", "zir", "]"}], "-", 
             RowBox[{"Log", "[", "zuv", "]"}]}], ")"}]}], "]"}]}], ",", " ", 
        RowBox[{"c", " ", "==", " ", 
         RowBox[{"1", "/", "2"}]}]}], "}"}]}], "\n", "\t", "}"}], "]"}]}], 
  ";"}]], "Input",
